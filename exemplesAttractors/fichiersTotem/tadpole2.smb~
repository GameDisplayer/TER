# exemple tirÃ© de Z. Khalis, J.-P. Comet, A. Richard, and G. Bernot. The SMBioNet Method for Discovering Models of Gene Regulatory Networks. Genes, Genomes and Genomics. 3(special issue 1):15-22, 2009.

VAR
T3 = 0 3 ;
T4 = 0 2 ;
d2 = 0 1 ;
d3 = 0 1 ;
gi = 0 1 ;
gp = 0 1 ;
gt = 0 1 ;
tr = 0 1 ;

REG
catalyse1 [((d2>=1)&(T4>=1))]=> T3 ;
catalyse3 [!((d3>=1)&(T3>=1))]=> T3 ;

catalyse2 [!((d3>=1)&(T4>=1))]=> T4 ;

factransc [(T3>=3)&(tr>=1)]=> gt ;

acti1 [(gi>=1)]  => d2 ;
inhi1 [!(gt>=1)]   => d2 tr ;


acti2 [(T3>=1)]  => d3 gi ;
inhi2 [!(gp>=1)]   => d3 ;

acti3 [(T3>=2)]   => gp tr ;

PARA
K_T3 = 1 ;

K_d3 = 0;
K_d3+acti2 = 0;
K_d3+inhi2 = 0;
K_d3+acti2+inhi2 = 0;

K_gi = 0;
K_gi+acti2 = 1;

K_gp = 0;
K_gp+acti3 = 1;

#K_gt = 0;
#K_gt+factransc = 0;

K_tr = 0;
K_tr+acti3 = 0;
K_tr+inhi1 = 0;
K_tr+acti3+inhi1 = 1;

CTL

# perte de la queue
((T3=3 & T4=1 & d2=1 & d3=0 & gi=1 & gp=1 & tr=1 & gt=0) -> AF(gt=1));


